<?xml version="1.0" encoding="UTF-8"?>
<!-- Car vehicle routes for traffic simulation -->
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    
    <!-- Vehicle Type Definition for Cars -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5.0" minGap="2.5" maxSpeed="55.0" color="1,0,0"/>
    
    <!-- Routes -->
    <route id="route_car_1" edges="1to2 2to3"/>
    <route id="route_car_2" edges="2to3 3to4"/>
    <route id="route_car_3" edges="3to4 4to1"/>
    <route id="route_car_4" edges="1to2 2to3 3to4"/>
    
    <!-- Vehicle Definitions -->
    <vehicle id="car_0" type="car" route="route_car_1" depart="0.00" color="1,0,0"/>
    <vehicle id="car_1" type="car" route="route_car_2" depart="5.00" color="1,0,0"/>
    <vehicle id="car_2" type="car" route="route_car_3" depart="10.00" color="1,0,0"/>
    <vehicle id="car_3" type="car" route="route_car_4" depart="15.00" color="1,0,0"/>
    <vehicle id="car_4" type="car" route="route_car_1" depart="20.00" color="1,0,0"/>
    <vehicle id="car_5" type="car" route="route_car_2" depart="25.00" color="1,0,0"/>
    
</routes>
